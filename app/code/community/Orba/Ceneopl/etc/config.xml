<?xml version="1.0"?>
<config>
    <modules>
        <Orba_Ceneopl>
            <version>1.1.2</version>
        </Orba_Ceneopl>
    </modules>
    <frontend>
        <routers>
            <ceneopl>
                <use>standard</use>
                <args>
                    <module>Orba_Ceneopl</module>
                    <frontName>ceneopl</frontName>
                </args>
            </ceneopl>
        </routers>
        <layout>
            <updates>
                <ceneopl>
                    <file>ceneopl.xml</file>
                </ceneopl>
            </updates>
        </layout>
        <translate>
            <modules>
                <Orba_Ceneopl>
                    <files>
                        <default>Orba_Ceneopl.csv</default>
                    </files>
                </Orba_Ceneopl>
            </modules>
        </translate>
    </frontend>  
    <global>
        <blocks>
            <ceneopl>
                <class>Orba_Ceneopl_Block</class>
            </ceneopl>
        </blocks>
        <models>
            <ceneopl>
                <class>Orba_Ceneopl_Model</class>
                <resourceModel>ceneopl_mysql4</resourceModel>
            </ceneopl>
            <ceneopl_mysql4>
                <class>Orba_Ceneopl_Model_Mysql4</class>
                <entities>
                    <category>
                        <table>orba_ceneo_category</table>
                    </category>
                    <mapping>
                        <table>orba_ceneo_mapping</table>
                    </mapping>
                    <mapping_catalog_category>
                        <table>orba_ceneo_mapping_catalog_category</table>
                    </mapping_catalog_category>
                </entities>
            </ceneopl_mysql4>
        </models>
        <resources>
            <ceneopl_setup>
                <setup>
                    <module>Orba_Ceneopl</module>
                    <class>Orba_Ceneopl_Model_Mysql4_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </ceneopl_setup>
            <ceneopl_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </ceneopl_write>
            <ceneopl_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </ceneopl_read>
        </resources>
        <helpers>
            <ceneopl>
                <class>Orba_Ceneopl_Helper</class>
            </ceneopl>
        </helpers>
        <cache>
            <types>
                <ceneopl module="ceneopl" translate="label description">
                    <label>Ceneo.pl</label>
                    <description>Ceneo.pl resources</description>
                    <tags>CENEOPL</tags>
                </ceneopl>
            </types>
        </cache>
    </global>
    <default>
        <ceneopl>
            <attr_other>
                <Producent>manufacturer</Producent>
            </attr_other>
        </ceneopl>
    </default>
    <adminhtml>
        <translate>
            <modules>
                <Orba_Ceneopl>
                    <files>
                        <default>Orba_Ceneopl.csv</default>
                    </files>
                </Orba_Ceneopl>
            </modules>
        </translate>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <ceneopl>
                                            <title>ORBA | Ceneo.pl</title>
                                        </ceneopl>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
        <layout>
            <updates>
                <ceneopl>
                    <file>ceneopl.xml</file>
                </ceneopl>
            </updates>
        </layout>
    </adminhtml>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <ceneopl before="Mage_Adminhtml">Orba_Ceneopl_Adminhtml</ceneopl>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>
